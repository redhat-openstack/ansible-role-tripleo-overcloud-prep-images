[project]
	description = An ansible role for tasks related to overcloud images. e.g. registration, introspection, upload etc.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group oooq-core
	owner = group user/wes hayutin (weshayutin)
[access "refs/for/refs/*"]
	read = group user/wes hayutin (weshayutin)
	push = group user/wes hayutin (weshayutin)
	label-Code-Review = -2..+2 group oooq-core
	label-Code-Review = -2..+2 group user/wes hayutin (weshayutin)
	label-Verified = -1..+1 group oooq-core
	label-Verified = -1..+1 group user/rdo-ci-hudson (rdo-ci-hudson)
	label-Verified = -1..+1 group user/rdo-ci-jenkins (rdo-ci-jenkins)
	label-Verified = -1..+1 group user/wes hayutin (weshayutin)
	rebase = group user/wes hayutin (weshayutin)
	removeReviewer = group user/wes hayutin (weshayutin)
	submit = group oooq-core
	submit = group user/wes hayutin (weshayutin)
[access "refs/heads/*"]
	read = group user/wes hayutin (weshayutin)
	create = group user/wes hayutin (weshayutin)
	push = +force group user/wes hayutin (weshayutin)
	pushMerge = group user/wes hayutin (weshayutin)
[access "refs/tags/*"]
	push = +force group user/wes hayutin (weshayutin)
	pushTag = group user/wes hayutin (weshayutin)
	pushSignedTag = group user/wes hayutin (weshayutin)
